{
    "name": "ketancit/citboilerplate",
    "description": "The boilerplate structure of the plugin for CSIT",
    "type": "project",
    "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
    "config": {
        "vendor-dir": "../../path/to/vendors"
    },
    "require": {
        "php": ">=7.1",
        "composer/installers": "~1.0",
        "fxp/composer-asset-plugin": "*"
    },
    "license": "mit",
    "authors": [
        {
            "name": "Ketan Vyawahare",
            "email": "ketanv@conscienceit.com",
            "homepage": "https://conscienceit.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
    "installer-paths": {
      "admin/": ["type:admin-class"],
      "public/": ["type:public-class"]
    }
  }
}
